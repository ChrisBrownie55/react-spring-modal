{
  "name": "workspaces",
  "private": true,
  "workspaces": [
    "./spring",
    "./framer",
    "./examples/*"
  ],
  "scripts": {
    "build": "yarn build:spring && yarn build:framer",
    "build:spring": "yarn workspace react-spring-modal build",
